<h4>Dashboard</h4>
<h3>Welcome {{name}}</h3>
<p>Here is my public story</p>

{{#if stories}}

<table class="striped">
    <thead>
        <tr>
            <th>Title</th>
            <th>Data</th>
            <th>Status </th>
            <th></th>
        </tr>
    </thead>

    <tbody>
        {{#each stories}}

        <tr>
            <td><a href="/stories/{{_id}}"></a>{{title}}</td>
            <td>{{createdAt}}</td>
            <td><span class="dash-status">{{status}}</span></td>
            <td></td>
        </tr>

        {{/each}}
    </tbody>


</table>


{{else}}
<p>You have not create any stories</p>

{{/if}}